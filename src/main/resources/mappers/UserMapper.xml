<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.application.model.repository">

    <insert id="save" parameterType="com.application.model.dto.UserDto">
        INSERT INTO user_tb ( seq
        , name )
        VALUES ( #{seq}
        , #{name} )
    </insert>

</mapper>